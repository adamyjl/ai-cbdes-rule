{
  "name": "ai-cbdes-rule",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.cjs",
    "build": "tsc -p tsconfig.json && vite build --config vite.config.cjs",
    "preview": "vite preview --config vite.config.cjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "check": "npm run typecheck",
    "api": "tsx api/index.ts",
    "api:dev": "tsx watch api/index.ts"
  },
  "dependencies": {
    "antd": "^5.22.7",
    "express": "^4.21.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^6.0.5"
  }
}
